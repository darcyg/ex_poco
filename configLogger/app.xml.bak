<?xml version="1.0" ?>
<Application>
	<logging>
		<channels>
			<c1>
				<class>ConsoleChannel</class>
			</c1>
			<c2>
				<class>FileChannel</class>
				<path>./test.log</path>
			</c2>
			<c3>
				<class>SplitterChannel</class>
				<channels>c1,c2</channels>
			</c3>
		</channels>
		<loggers>
			<root>
				<name></name>
				<channel>c1</channel>
				<level>trace</level>
			</root>
			<app>
				<name>Application</name>
				<channel>c1</channel>
				<level>trace</level>
				<formatter>
					<class>PatternFormatter</class>
					<pattern>%L%H-%M-%S.%i %t</pattern>
				</formatter>
			</app>
		</loggers>
	</logging>
</Application>
